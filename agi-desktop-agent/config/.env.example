# API Keys (get these first!)
GROQ_API_KEY=your_groq_api_key_here
LINKUP_API_KEY=your_linkup_api_key_here

# Model Configuration
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_TEMPERATURE=0.7
MAX_TOKENS=4096

# Linkup Configuration
LINKUP_DEPTH=standard
LINKUP_OUTPUT_TYPE=searchResults

# Google API Configuration
# Set to true to enable Gmail and Google Calendar integration
ENABLE_GOOGLE_APIS=false
GOOGLE_CREDENTIALS_FILE=config/google_credentials.json
GOOGLE_TOKEN_CACHE_DIR=data/.google_tokens

# Memory Configuration
MEMORY_DIR=data/
CONVERSATION_FILE=data/conversations/history.json
EMAIL_CACHE_FILE=data/emails/cache.json

# App Configuration
APP_TITLE=AGI Desktop Intelligence Agent
DEBUG_MODE=true
